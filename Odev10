--city tablosu ile country tablosunda bulunan şehir (city) ve ülke (country) isimlerini birlikte görebileceğimiz LEFT JOIN sorgusunu yazınız.
SELECT CITY, COUNTRY FROM CITY LEFT JOIN COUNTRY on CITY.COUNTRY_ID = COUNTRY.COUNTRY_ID;
--customer tablosu ile payment tablosunda bulunan payment_id ile customer tablosundaki first_name ve last_name isimlerini birlikte görebileceğimiz RIGHT JOIN sorgusunu yazınız.
SELECT FIRST_NAME, LAST_NAME FROM PAYMENT RIGHT JOIN CUSTOMER ON PAYMENT.CUSTOMER_ID=CUSTOMER.CUSTOMER_ID
--customer tablosu ile rental tablosunda bulunan rental_id ile customer tablosundaki first_name ve last_name isimlerini birlikte görebileceğimiz FULL JOIN sorgusunu yazınız.
SELECT FIRST_NAME, LAST_NAME FROM RENTAL FULL OUT JOIN CUSTOMER ON RENTAL.CUSTOMER_ID=CUSTOMER.CUSTOMER_ID
